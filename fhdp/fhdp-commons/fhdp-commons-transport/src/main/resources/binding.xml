<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" xmlns="http://java.sun.com/xml/ns/jaxws"
			   xmlns:annox="http://annox.dev.java.net"
               xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd http://annox.dev.java.net"
               jaxb:extensionBindingPrefixes="xjc annox"
               version="2.1">

	<jaxb:globalBindings>
		<jaxb:serializable uid="12345678900"/>
		<xjc:javaType name="java.lang.Boolean" xmlType="xs:boolean"
					  adapter="pl.fhframework.dp.commons.jaxb.BooleanAdapter"/>
		<xjc:javaType name="java.time.LocalDate" xmlType="xs:date"
					  adapter="pl.fhframework.dp.commons.jaxb.XSLocalDateAdapter" />
		<xjc:javaType name="java.time.LocalDateTime" xmlType="xs:dateTime"
					  adapter="pl.fhframework.dp.commons.jaxb.XSLocalDateTimeAdapter" />
		<xjc:javaType name="java.lang.Long" xmlType="xs:integer"
					  adapter="pl.fhframework.dp.commons.jaxb.XSLongAdapter"/>
	</jaxb:globalBindings>

	<jaxb:bindings schemaLocation="xsd/DocOutline.xsd">
		<jaxb:schemaBindings>
			<jaxb:package name="pl.fhframework.dp.commons.fh.outline"></jaxb:package>
		</jaxb:schemaBindings>
	</jaxb:bindings>

</jaxb:bindings>
